<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ucucite.edu.epass_admin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/epasslogo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config_other"
        android:roundIcon="@drawable/epasslogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.EpassAdmin"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UsersEditorActivity" />
        <activity android:name=".UsersMainActivity" />
        <activity android:name=".ChooseSession" />
        <activity android:name=".Login">

        </activity>
        <activity android:name=".AdminEditorActivity" />
        <activity android:name=".AdminMainActivity" />
    </application>

</manifest>